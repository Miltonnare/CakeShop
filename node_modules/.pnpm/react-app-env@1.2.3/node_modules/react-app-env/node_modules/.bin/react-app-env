#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fred/Desktop/shop/node_modules/.pnpm/react-app-env@1.2.3/node_modules/react-app-env/bin/node_modules:/home/fred/Desktop/shop/node_modules/.pnpm/react-app-env@1.2.3/node_modules/react-app-env/node_modules:/home/fred/Desktop/shop/node_modules/.pnpm/react-app-env@1.2.3/node_modules:/home/fred/Desktop/shop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fred/Desktop/shop/node_modules/.pnpm/react-app-env@1.2.3/node_modules/react-app-env/bin/node_modules:/home/fred/Desktop/shop/node_modules/.pnpm/react-app-env@1.2.3/node_modules/react-app-env/node_modules:/home/fred/Desktop/shop/node_modules/.pnpm/react-app-env@1.2.3/node_modules:/home/fred/Desktop/shop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-app-env" "$@"
else
  exec node  "$basedir/../../bin/react-app-env" "$@"
fi
